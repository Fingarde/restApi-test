    "get_player_uuid_post": {
        "type": "post",
        "description": "Get player's informations by uuid",
        "url": "/server/players/uuid/{uuid}"
    },
    "get_player_put": {
        "type": "put",
        "description": "Get player's informations by uuid",
        "url": "/server/players/uuid/{uuid}"
